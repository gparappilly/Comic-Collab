<html>

<head>
    <title></title>
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" />
</head>
<style>
body  {
    background: linear-gradient(rgb(128, 255, 128), rgba(255,0,0,0));
}
</style>
<body>
<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <a class="navbar-brand" href="../home">ComicCollab</a>
    </div>
</nav>
    <div class="container">
        <br>
        <div class="page-header" style="margin-top: 60px">
            <h1>Userlist</h1>
        </div>
        <ol>
            <% userlist.forEach(function(user) { %>
            <li><a href = "users/<%= user.username %>"><img style="height:25px; width:25px;" src = "<%= user.profilepicture %>"> <%= user.username %></a></li>
            <% }); %>
        </ol>
    </div>
    <br>
</body>

</html>